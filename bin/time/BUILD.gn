import("//build/package.gni")

executable("bin") {
  output_name = "time"

  sources = [
    "time.c",
  ]

  deps = [
    "//zircon/public/lib/fdio",
  ]

  libs = [ "zircon" ]
}

package("time") {
  deps = [
    ":bin",
  ]

  binaries = [
    {
      name = "time"
      shell = true
    },
  ]
}
